{
  "verbose": true,
  "transform": {
    ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/test/.*[.](test|spec))\\.(ts|tsx|js)$",
  "testPathIgnorePatterns": [
    "<rootDir>[/\\\\](dist|coverage|node_modules)[/\\\\]",
    "_\\w*.\\w+$"
  ],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ]
}